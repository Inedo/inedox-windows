<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>Windows</AssemblyName>
    <RootNamespace>Inedo.Extensions.Windows</RootNamespace>
    <OutputType>Library</OutputType>
    <TargetFramework>net452</TargetFramework>
    <LangVersion>latest</LangVersion>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System.ServiceProcess" />
    <Reference Include="System.Web" />
    <PackageReference Include="System.Management.Automation_PowerShell_3.0" Version="6.3.9600.17400" />
    <PackageReference Include="Microsoft.Web.Administration" Version="7.0.0.0" />
    <PackageReference Include="Inedo.SDK" Version="1.7.2">
      <ExcludeAssets>runtime</ExcludeAssets>
    </PackageReference>
  </ItemGroup>
</Project>
